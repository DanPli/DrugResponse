<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <!-- ====== Compatability Patch ==================== -->
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>[1.0] RimCuisine 2: Smokes and Shrooms Expansion</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/ThingDef[defName="RC2_Dreamstick" or defName="RC2_Skag" or defName="RC2_Tango" or defName="RC2_Viko" or defName="RC2_Zope"]</xpath>
          <value>
            <li Class="MSPainless.MSPainDrug">
              <ManagesPain>true</ManagesPain>
            </li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>